{
  "name": "react-native-worklets-core",
  "version": "1.6.2",
  "summary": "A Worklet runner for React Native",
  "homepage": "https://github.com/margelo/react-native-worklets-core#readme",
  "license": "MIT",
  "authors": "Christian Falch <christian.falch@gmail.com> (https://github.com/chrfalch)",
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://github.com/margelo/react-native-worklets-core.git",
    "tag": "1.6.2"
  },
  "source_files": [
    "ios/**/*.{h,m,mm}",
    "cpp/**/*.{h,cpp}"
  ],
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "HEADER_SEARCH_PATHS": [
      "\"$(PODS_ROOT)/Headers/Private/Yoga\"",
      "$(PODS_ROOT)/glog",
      "$(PODS_ROOT)/boost",
      "$(PODS_ROOT)/DoubleConversion",
      "$(PODS_ROOT)/fast_float/include",
      "$(PODS_ROOT)/fmt/include",
      "$(PODS_ROOT)/SocketRocket",
      "$(PODS_ROOT)/RCT-Folly"
    ],
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
    "OTHER_CPLUSPLUSFLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED=1 ",
    "OTHER_SWIFT_FLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED"
  },
  "compiler_flags": "-DRCT_NEW_ARCH_ENABLED=1",
  "dependencies": {
    "React-Core": [],
    "React-RCTFabric": [],
    "ReactCodegen": [],
    "RCTRequired": [],
    "RCTTypeSafety": [],
    "ReactCommon/turbomodule/bridging": [],
    "ReactCommon/turbomodule/core": [],
    "React-NativeModulesApple": [],
    "Yoga": [],
    "React-Fabric": [],
    "React-graphics": [],
    "React-utils": [],
    "React-featureflags": [],
    "React-debug": [],
    "React-ImageManager": [],
    "React-rendererdebug": [],
    "React-jsi": [],
    "React-renderercss": [],
    "hermes-engine": [],
    "glog": [],
    "boost": [],
    "DoubleConversion": [],
    "fast_float": [],
    "fmt": [],
    "RCT-Folly": [],
    "SocketRocket": [],
    "RCT-Folly/Fabric": []
  }
}
